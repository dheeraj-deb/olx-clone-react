ntext(FirebaseContext)\n  useEffect(()=>{\n    const {userId}=postDetails\n    firebase.firestore().collection('users').where('id','==',userId).get().then((res)=>{\n      res.forEach(doc => {\n        setUserDetails(doc.data())\n      });\n    })\n  },[])\n\n  return (\n    <div className=\"viewParentDiv\">\n      <div className=\"imageShowDiv\">\n        <img\n          src={postDetails.url}\n          alt=\"\"\n        />\n      </div>\n      <div className=\"rightSection\">\n        <div className=\"productDetails\">\n          <p>&#x20B9; {postDetails.price} </p>\n          <span>{postDetails.name}</span>\n          <p>{postDetails.category}</p>\n          <span>{postDetails.createdAt}</span>\n        </div>\n        {userDetails &&\n        <div className=\"contactDetails\">\n          <p>Seller details</p>\n          <p>{userDetails.username}</p>\n          <p>{userDetails.phone}</p>\n        </div>\n        }\n      </div>\n    </div>\n  );\n}\nexport default View;\n","/home/dheeraj/Documents/olx-copy1/Olx-Clone-React/src/Components/Header/Header.js",[],"/home/dheeraj/Documents/olx-copy1/Olx-Clone-React/src/Components/Banner/Banner.js",[],"/home/dheeraj/Documents/olx-copy1/Olx-Clone-React/src/Components/Posts/Posts.js",["105"],"import React,{useEffect,useContext,useState} from 'react';\n\nimport Heart from '../../assets/Heart';\nimport { FirebaseContext } from '../../Store/FirebaseContext';\nimport {PostContext} from '../../Store/PostContext'\nimport './Post.css';\nimport {useHistory} from 'react-router-dom'\n\nfunction Posts() {\nconst {firebase}=useContext(FirebaseContext)\nconst [products,setProducts]=useState([])\nconst {setPostDetails}=useContext(PostContext)\nconst history=useHistory()\nuseEffect(()=>{\n  firebase.firestore().collection('products').get().then((snapshot)=>{\n    const allPost=snapshot.docs.map((product)=>{\n      return {\n        ...product.data(),\n        id:product.id\n      }\n    })\n    setProducts(allPost)\n  })\n},[])\n  return 